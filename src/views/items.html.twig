<div class="form-group">
    <div class="col-sm-4 col-sm-offset-4" id="itemList">
        <select class="form-control" name="items" id="items">
            <option></option>
            {% for item in items %}
                <option value="{{ item.ID }}">{{ item.DESCRIPTION }}</option>
            {% endfor %}
        </select>
    </div>
</div>
